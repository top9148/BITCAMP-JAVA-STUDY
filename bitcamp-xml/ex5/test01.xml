<?xml version="1.0" encoding="UTF-8"?>
<!-- 네임스페이스
- 태그를 그룹으로 묶을 때 사용하는 문법이다.
- 태그 관리를 쉽게 하기 위해 사용한다.

네임스페이스 정의 문법
  <태그명 xmlns:별명="네임스페이스명">
- 네임스페이스명 보통 URL로 표현한다.

네임스페이스 사용 문법
- 네임스페이스별명:태그명
- 네임스페이스별명:속성명

-->
<b:books xmlns:b="http://bitcamp.co.kr/java100/books"
         xmlns:a="http://bitcamp.co.kr/java100/author"
         xmlns:p="http://bitcamp.co.kr/java100/press">
         
    <a:author a:id="a001" a:name="홍길동"/>
    <a:author a:id="a002" a:name="임꺽정"/>
    <a:author a:id="a003" a:name="유관순"/>
    
    <p:press p:id="p001" b:name="비트출판사"/>
    <p:press p:id="p002" b:name="위키북스"/>
    <p:press p:id="p003" b:name="프리렉"/>
    
    <b:book b:name="aaaa" b:press="p001" b:author="a003"/>
    
</b:books>






